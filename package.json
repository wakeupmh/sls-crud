{
  "name": "grocery-store",
  "version": "1.0.0",
  "description": "grocery store API",
  "devDependencies": {
    "@biomejs/biome": "~2.1.1",
    "@evilmartians/lefthook": "~1.12.2",
    "@types/aws-lambda": "~8.10.150",
    "@types/jest": "~30.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "~3.2.4",
    "dotenv": "~17.2.0",
    "jest": "~30.0.4",
    "serverless-offline": "~14.4.0",
    "ts-jest": "~29.4.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.8.3",
    "vite": "~5.4.0",
